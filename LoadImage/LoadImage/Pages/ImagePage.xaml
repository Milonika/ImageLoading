<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="LoadImage.ImagePage" BackgroundColor="White" Visual="Material">
    <ContentPage.Content>
        <StackLayout>
            <Entry x:Name="nameentry" BackgroundColor="lightGray" PlaceholderColor="Gray" IsEnabled="False" Margin="40, 40, 40, 0" FontSize="15"/>
            <Image x:Name="imagebox" Margin="40" HeightRequest="500" MinimumHeightRequest="500"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>